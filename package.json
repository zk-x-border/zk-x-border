{
  "name": "zk-email",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@openzeppelin/contracts": "3.4.2-solc-0.7",
    "@uniswap/v3-periphery": "^1.4.3",
    "chai": "^4.3.7",
    "circom_tester": "^0.0.19",
    "circomlib": "^2.0.5",
    "dotenv": "^16.0.3",
    "mocha": "^10.2.0",
    "snarkjs": "0.4.22"
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "scripts": {
    "start": "vite",
    "chain": "npx hardhat node",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest --runInBand --testPathIgnorePatterns='e2e' --reporters=default --reporters=jest-junit",
    "start-e2e-test-server": "serve -s dist -p 3000",
    "test:e2e-ui": "CI=true jest ./src/e2e-ui*.test.*",
    "test:e2e-zkp": "CI=true jest ./src/e2e-dl-zkp.test.ts",
    "jest": "jest",
    "compile": "node circuits/scripts/compile.js",
    "gen-input": "npx tsx ./src/scripts/generate_input.ts",
    "compile-all": "yarn gen-input && yarn compile email true",
    "prepare": "husky install",
    "compileContracts": "npx hardhat compile",
    "testContracts": "npx hardhat test --network hardhat",
    "demo-1": "yarn hardhat run contracts/scripts/1_claim_order.js",
    "demo-2": "yarn hardhat run contracts/scripts/2_complete_order.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie <= 99",
      "not android <= 4.4.4",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.21.5",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@types/atob": "^2.1.2",
    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.0.6",
    "@types/node-forge": "^1.3.2",
    "@types/tar-stream": "^2.2.2",
    "babel-preset-jest": "^29.5.0",
    "browserify-fs": "^1.0.0",
    "browserstack-local": "^1.5.1",
    "browserstack-node-sdk": "^1.6.1",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.0.0",
    "hardhat": "^2.12.2",
    "hardhat-deploy": "^0.11.25",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^15.0.0",
    "madge": "^6.0.0",
    "msw": "^1.0.1",
    "nodemon": "^2.0.19",
    "process": "^0.11.10",
    "puppeteer": "18.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "selenium-webdriver": "^4.8.1",
    "ts-node-dev": "^2.0.0",
    "util": "^0.12.5",
    "vite-plugin-commonjs": "^0.7.1",
    "vite-plugin-ngmi-polyfill": "^0.0.2",
    "vite-plugin-node-polyfills": "^0.8.2"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@rainbow-me/rainbowkit)"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./src/setupTests.js",
      "jsdom-worker"
    ]
  },
  "packageManager": "yarn@3.2.3"
}
